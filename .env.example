# Environment Configuration
NODE_ENV=development

# PostgreSQL Database Configuration
# Replace with your database credentials
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=public

# Prisma Database URL
# Format: postgresql://USER:PASSWORD@HOST:PORT/DATABASE
PRISMA_DB_URL=postgresql://your_postgres_user:your_secure_password@localhost:5432/public

# AWS S3 Configuration
# Get these from AWS IAM User Credentials
S3_ACCESS_KEY_ID=ACCESS_KEY_ID_HERE
S3_SECRET_ACCESS_KEY=SECRET_ACCESS_KEY_HERE
AWS_BUCKET=BUCKET_NAME_HERE # Replace as necessary
AWS_REGION=us-east-1 # Replace as necessary

# AWS URLs Configuration
# Replace with your CloudFront and S3 URLs
CDN_HOSTNAME=123455667Ssdjah.cloudfront.net
CDN_URL=https://123455667Ssdjah.cloudfront.net
ORIGIN_HOSTNAME=123455667Ssdjah.s3.us-east-1.amazonaws.com
ORIGIN_URL=https://123455667Ssdjah.s3.us-east-1.amazonaws.com

# Discord Bot Configuration
# Get this from Discord Developer Portal
DISCORD_TOKEN=DISCORD_BOT_TOKEN_HERE
